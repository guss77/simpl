#===============================================
# Default targets
# This makefile is only used to clean specific
# files in the simpl/bin directory
#===============================================
clean:\
     title
	@-rm fclogger
	@-rm fcshow
	@-rm fcslay
	@-rm tclSurrogate
	@-rm protocolRouter
	@-rm surrogateTcp
	@-rm dumpProtocolTable
	@echo simpl/bin clean

title:
	@echo simpl/bin clean starting
